{
  "name": "brutalist-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:db": "jest --config jest.db.config.js",
    "test:db:watch": "jest --config jest.db.config.js --watch",
    "test:coverage": "jest --coverage",
    "test:db:coverage": "jest --config jest.db.config.js --coverage",
    "test:unit": "jest --coverage --passWithNoTests",
    "test:integration": "jest --config jest.db.config.js --coverage --passWithNoTests",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:accessibility": "playwright test --grep=\"accessibility\"",
    "test:visual": "playwright test visual-regression",
    "test:visual:update": "playwright test visual-regression --update-snapshots",
    "test:all": "node scripts/test-runner.js",
    "test:ci": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:quick": "jest --passWithNoTests --bail --findRelatedTests",
    "test:changed": "jest --passWithNoTests --onlyChanged",
    "test:storybook": "test-storybook",
    "test:storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run build-storybook && npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && npm run test:storybook\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset",
    "db:test:setup": "DATABASE_URL=$TEST_DATABASE_URL prisma db push",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf .next && rm -rf node_modules/.cache && rm -rf test-results && rm -rf coverage",
    "clean:test": "rm -rf test-results && rm -rf coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps",
    "analyze": "ANALYZE=true npm run build && node scripts/analyze-bundle.js",
    "deploy": "node scripts/deploy.js",
    "deploy:staging": "NODE_ENV=staging node scripts/deploy.js",
    "deploy:production": "NODE_ENV=production node scripts/deploy.js",
    "config:production": "node scripts/production-config.js",
    "lighthouse": "lhci autorun",
    "lighthouse:ci": "lhci autorun --upload.target=temporary-public-storage",
    "performance:monitor": "node -e \"require('./src/lib/performance-monitor.js').performanceMonitor.startMonitoring()\"",
    "cache:warm": "node -e \"require('./src/lib/cache.js').warmCriticalCaches()\"",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@octokit/rest": "^22.0.0",
    "@prisma/client": "^6.13.0",
    "@react-three/drei": "^10.6.1",
    "@react-three/fiber": "^9.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/three": "^0.179.0",
    "@uploadthing/react": "^7.3.2",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.539.0",
    "next": "15.4.6",
    "next-auth": "^5.0.0-beta.29",
    "prisma": "^6.13.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "react-syntax-highlighter": "^15.6.1",
    "reading-time": "^1.5.0",
    "recharts": "^3.1.2",
    "resend": "^6.0.1",
    "tailwind-merge": "^3.3.1",
    "three": "^0.179.1",
    "uploadthing": "^7.7.3",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@chromatic-com/storybook": "^4.1.0",
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^9.3.0",
    "@storybook/addon-a11y": "^9.1.1",
    "@storybook/addon-docs": "^9.1.1",
    "@storybook/addon-onboarding": "^9.1.1",
    "@storybook/addon-vitest": "^9.1.1",
    "@storybook/nextjs-vite": "^9.1.1",
    "@storybook/test-runner": "^0.23.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "axe-core": "^4.10.3",
    "chromatic": "^13.1.3",
    "concurrently": "^9.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "eslint-plugin-storybook": "^9.1.1",
    "http-server": "^14.1.1",
    "jest": "^30.0.5",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "jest-environment-node": "^30.0.5",
    "jest-mock-extended": "^4.0.0",
    "playwright": "^1.54.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "storybook": "^9.1.1",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.1"
  }
}
